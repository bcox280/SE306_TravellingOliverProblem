<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox prefWidth="1715.0" style="-fx-background-color: #424242;" xmlns="http://javafx.com/javafx/10.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="uoa.se306.travellingoliverproblem.visualiser.FXController">
    <children>
        <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="850.0"
              prefWidth="1200.0">
            <children>
                <AnchorPane prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Text fill="WHITE" layoutX="14.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0"
                              text="Input DAG">
                            <font>
                                <Font name="Roboto Medium" size="24.0"/>
                            </font>
                        </Text>
                        <Text fx:id="scheduleTitleText" fill="WHITE" layoutX="385.0" layoutY="35.0" strokeType="OUTSIDE"
                              strokeWidth="0.0" text="Current Known Best Schedule">
                            <font>
                                <Font name="Roboto Medium" size="24.0"/>
                            </font>
                        </Text>
                        <AnchorPane fx:id="statusPane" layoutX="909.0" layoutY="-1.0" prefHeight="50.0"
                                    prefWidth="291.0" style="-fx-background-color: orange;">
                            <children>
                                <Text fx:id="statusText" fill="WHITE" layoutX="18.0" layoutY="33.0" strokeType="OUTSIDE"
                                      strokeWidth="0.0" text="Starting up...">
                                    <font>
                                        <Font name="Roboto Bold" size="20.0"/>
                                    </font>
                                </Text>
                            </children>
                        </AnchorPane>
                    </children>
                </AnchorPane>
                <HBox maxWidth="1200.0" prefHeight="850.0">
                    <children>
                        <ScrollPane fx:id="graphScrollPane" hbarPolicy="NEVER" maxHeight="850.0" prefHeight="850.0"
                                    prefWidth="380.0" stylesheets="@scrollpane.css" vbarPolicy="NEVER">
                            <content>
                                <Pane fx:id="graphPane"/>
                            </content>
                        </ScrollPane>
                        <ScrollPane prefHeight="850.0" prefWidth="820.0" stylesheets="@scrollpane.css">
                            <content>
                                <Pane fx:id="schedulePane" prefWidth="800.0"/>
                            </content>
                        </ScrollPane>
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox fx:id="tilesBox" prefHeight="800.0" prefWidth="515.0"/>
    </children>
</HBox>
